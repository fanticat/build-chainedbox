diff --git a/arch/arm64/boot/dts/rockchip/rk3328-chainedbox-1512mhz.dts b/arch/arm64/boot/dts/rockchip/rk3328-chainedbox-1512mhz.dts
new file mode 100755
index 000000000..a4f9c1733
--- /dev/null
